site_name: Arrowhead Guide
theme:
  name: material
  language: sl
  palette:
    primary: indigo
    accent: deep orange
  font:
    text: Roboto
    code: Roboto Mono
  favicon: img/field.ico

nav:
  - En:
      - Arrowhead Shooting Guide: en/index.md
  #    - Equipment: en/getting-started.md
  #    - Shooting Technique: en/techniques.md
  - Sl:
      - Namesto uvoda: sl/getting-started.md
      - Razdalje: sl/distances.md
      - Ocenjevanje razdalj: sl/estimation.md
      - Test: sl/test.md
      - Poševni met: sl/graph.md
      - Kavč: sl/sofa.md
      - Skrivnostni obrat: sl/mystery.md

  #- Sr:
  #    - Arrowhead Guide: sr/index.md 

plugins:
  - search
  # - inline-svg-mod
  - include-markdown


extra_css:
  - stylesheets/extra.css
  
extra_javascript:
  - js/language_switcher.js

extra:
  alternate:
    - name: 🇬🇧 English
      link: /en/
    - name: 🇸🇮 Slovenščina
      link: /sl
    - name: 🇷🇸 Srpski
      link: /sr
